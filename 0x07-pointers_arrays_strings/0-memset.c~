#include "holberton.h"
#include <stdio.h>
#include <string.h>

/**
 *_memset - fills memory with a constant byte
 *
 *@s: pointer to memory area
 *@b: constant byte
 *@n: number of bytes of the memory
 *
 *return: pointer to the memory area s
 */

char *_memset(char *s, char b, unsigned int n)
{

	memset(s, b, n);
	return (s);
}
